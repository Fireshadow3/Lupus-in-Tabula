<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fastame.formichella.lupusintabula">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/master_chief"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".gameScreen" />
        <activity android:name=".signUp_activity" />
        <activity android:name=".MainMenu" />
        <activity android:name=".options" />
        <activity android:name=".chooseGame" />
        <activity android:name=".Main" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ENDING"></activity>
    </application>

</manifest>